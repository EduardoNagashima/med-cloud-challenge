// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  birthdate    DateTime
  uf           String
  city         String
  street       String
  number       Int
  neighborhood String
  zipCode      Int

  @@map("patients")
}
